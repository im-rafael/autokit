{
	"servers": {
		"serena": {
			"type": "stdio",
			"command": "uvx",
			"args": [
				"--from",
				"git+https://github.com/oraios/serena",
				"serena",
				"start-mcp-server",
				"serena==latest",
				"--context",
				"ide-assistant"
			]
		},
		"playwright": {
			"type": "stdio",
			"command": "npx",
			"args": [
				"@playwright/mcp@latest"
			]
		},
		"dbhub": {
			"type": "stdio",
			"command": "npx",
			"args": [
				"@bytebase/dbhub@0.13.2",
				"--transport",
				"stdio",
				"--dsn",
				""
			]
		},
		"context7": {
			"type": "stdio",
			"command": "npx",
			"args": [
				"@upstash/context7-mcp@1.0.31"
			],
		}
	},
	"inputs": []
}